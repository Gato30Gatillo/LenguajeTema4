package es.iesjandula.coches;

import java.util.Arrays;

public class Coche extends Motor 
{

	private String modelo;
	private String marca;
	private Motor motor;
	private String kilometros;

	public Coche(int caballos, String tipo, String modelo, String marca, Motor motor, String kilometros) 
	{
		super(caballos, tipo);
		this.modelo = modelo;
		this.marca = marca;
		this.motor = motor;
		this.kilometros = kilometros;
	}
	
	public String getModelo() 
	{
		return this.modelo;
	}
	public void setModelo(String modelo) 
	{
		this.modelo = modelo;
	}
	public String getMarca() 
	{
		return this.marca;
	}
	public void setMarca(String marca) 
	{
		this.marca = marca;
	}
	public String getKilometros() 
	{
		return this.kilometros;
	}
	public void setKilometros(String kilometros) 
	{
		this.kilometros = kilometros;
	}
	@Override
	public String toString() {
		return "Coche [modelo=" + modelo + ", marca=" + marca + ", motor=" + motor + ", kilometros=" + kilometros + "]";
	}
	
}	

